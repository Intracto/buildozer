src_base_path: ./
dest_base_path: ./
postcss:
  - name: null
    options: null
scss:
  - src: scss/**/*.scss
    dest: dest/css
img:
  - src: img/**/*.{png,jpg,jpeg,gif,svg}
    dest: dest/img
js:
  - src: js/**/*.js
    dest: dest/js
js-concat:
  - src: js/concat/*.js
    name: all.js
    dest: dest/js
svg-sprite:
  - src: img/sprite/*.svg
    name: sprite.svg
    dest: dest/img/sprite
browsersync:
  server: null # Static sites
  proxy: null # Dynamic sites
